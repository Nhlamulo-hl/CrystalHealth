<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GetStarted">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="507dp"
        android:layout_height="814dp"

        android:background="@drawable/outline"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/gradient" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="212dp"
        android:layout_height="223dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="292dp"
        android:layout_height="383dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="12dp"
        android:rotationY="72"
        app:layout_constraintBottom_toTopOf="@+id/imageView14"
        app:layout_constraintEnd_toEndOf="@+id/imageView12"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.7"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="297dp"
        android:layout_height="416dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="32dp"
        android:rotationY="-72"
        app:layout_constraintBottom_toTopOf="@+id/imageView15"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="121dp"
        android:layout_height="118dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintEnd_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.571"
        app:srcCompat="@drawable/doctor" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="204dp"
        android:layout_height="199dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="331dp"
        android:layout_height="352dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="32dp"
        android:rotation="0"
        android:rotationY="72"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/imageView16"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="352dp"
        android:layout_height="268dp"
        android:layout_marginStart="4dp"
        android:rotationY="-72"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView16"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:layout_constraintVertical_bias="0.693"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="121dp"
        android:layout_height="130dp"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView13"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/imageView13"
        app:layout_constraintTop_toTopOf="@+id/imageView13"
        app:srcCompat="@drawable/multiple_users_silhouette" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="215dp"
        android:layout_height="136dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/inknut_antiqua_medium"
        android:text="A quick,trustworthy source which only has your best interests at Heart well should I say Health- CrystalHealth"
        android:textColor="#F6F2F2"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

</androidx.constraintlayout.widget.ConstraintLayout>