<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".MedicationView">


    <Button
        android:id="@+id/button10"
        android:layout_width="123dp"
        android:layout_height="48dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="158dp"
        android:layout_marginTop="679dp"

        android:layout_marginEnd="130dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@drawable/button"
        android:text="Billing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView61"
        android:layout_width="421dp"
        android:layout_height="365dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="200dp"
        android:alpha="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/caduceus" />

    <ImageView
        android:id="@+id/imageView59"
        android:layout_width="242dp"
        android:layout_height="229dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="259dp"
        android:layout_marginEnd="90dp"
        android:alpha="0.5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView61"
        app:srcCompat="@drawable/lotus__1_" />

    <ImageView
        android:id="@+id/profile_image"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="60dp"
        android:src="@drawable/facebook"
        app:civ_border_color="#FF000000"
        app:civ_border_width="2dp" />


    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_image"
        android:layout_centerHorizontal="true"
        android:textAlignment="center"
        android:layout_marginTop="50dp"
        android:textColor="@color/black"
        android:textSize="40dp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:fontFamily="@font/inknut_antiqua_medium"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="12dp" />

    <ImageView
        android:id="@+id/imageView90"
        android:layout_width="81dp"
        android:layout_height="64dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="330dp"
        android:background="@drawable/hp2"
        app:srcCompat="@drawable/left_arrow" />


</RelativeLayout>